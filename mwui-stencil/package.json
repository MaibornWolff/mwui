{
  "name": "@maibornwolff/mwui-stencil",
  "version": "0.3.0",
  "description": "Component Library using Figma's design tokens",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/mwui/mwui.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaibornWolff/mwui.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs --docs-json docs.json",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test:spec": "stencil test --spec",
    "test:spec:coverage": "stencil test --spec --coverage",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "format": "pretty-quick",
    "format:quick": "pretty-quick --staged",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006 -s www/build",
    "build-storybook": "build-storybook public -o ../docs -s www/build",
    "build-storybook:ci": "build-storybook",
    "release": "release-it"
  },
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@maibornwolff/mwui-stencil": "^0.1.12",
    "@stencil/angular-output-target": "^0.5.0",
    "@stencil/core": "^3.1.0",
    "@stencil/react-output-target": "^0.4.0",
    "@stencil/vue-output-target": "^0.7.0",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "prettier": "^2.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@popperjs/core": "^2.11.6",
    "@pxtrn/storybook-addon-docs-stencil": "^6.4.1",
    "@stencil-community/eslint-plugin": "^0.5.0",
    "@stencil/sass": "^3.0.0",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addons": "^6.5.10",
    "@storybook/html": "^6.1.21",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.10",
    "@types/jest": "^27.0.3",
    "@types/mdx": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-loader": "^9.1.2",
    "eslint": "^8.41.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.6.8",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fs-extra": "^11.1.0",
    "glob-promise": "^6.0.1",
    "husky": "^8.0.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "lint-staged": "^13.1.0",
    "pretty-quick": "^3.1.3",
    "puppeteer": "^19.10.1",
    "stencil-click-outside": "^1.8.0",
    "storybook-addon-turbo-build": "^1.1.0",
    "svgson": "^5.2.1",
    "typescript": "^4.9.4"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MaibornWolff/mwui/issues"
  },
  "homepage": "https://github.com/MaibornWolff/mwui#readme",
  "keywords": [
    "stencil",
    "web-components",
    "component",
    "library",
    "storybook",
    "design",
    "system"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Martin Prinz"
}
